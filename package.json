{
  "name": "teacher-tutor",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier . --write"
  },
  "dependencies": {
    "next": "13.5.5",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "13.5.5",
    "husky": "^8.0.0",
    "lint-staged": "^15.0.1",
    "prettier": "3.0.3",
    "typescript": "^5"
  },
  "lint-staged": {
    "!*.{js,ts,tsx}": "prettier --ignore-unknown --write",
    "*.{js,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
